<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- DE, by Sascha Hlusiak -->

    <CheckBoxPreference
        android:defaultValue="true"
        android:key="displaySeeds"
        android:summary="Felder markieren, an denen Anlegen möglich ist."
        android:title="Anlegepunkte zeigen" />
    <CheckBoxPreference
        android:defaultValue="true"
        android:key="ai"
        android:summary="Computer übernimmt Grün, Blau und Gelb"
        android:title="Computergegner" />
    <ListPreference
        android:defaultValue="0"
        android:dialogTitle="Spielstärke auswählen"
        android:entries="@array/aiLevelNames"
        android:entryValues="@array/aiLevelValues"
        android:key="aiLevel"
        android:persistent="true"
        android:summary="%s"
        android:title="Spielstärke" />
</PreferenceScreen>
